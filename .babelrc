{
  "env": {
    "test": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "browsers": [
                "ie >=11"
              ]
            }
          }
        ]
      ]
    },
    "development": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "browsers": [
                "ie >=11"
              ]
            }
          }
        ]
      ],
      "plugins": [
        [
          "transform-string-literal-replace",
          {
            "<MODULE_VERSION>": "CJS"
          }
        ]
      ]
    },
    "esm": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "browsers": [
                "ie >=11"
              ]
            },
            "modules": false
          }
        ]
      ],
      "plugins": [
        [
          "transform-string-literal-replace",
          {
            "<MODULE_VERSION>": "ESM"
          }
        ]
      ]
    }
  }
}
