{
  "name": "@howto/browser-modules",
  "version": "0.0.3",
  "description":
    "A sample repo demonstrating package.json config for a browser module published to NPM",
  "author": {
    "name": "Josh Crowther",
    "email": "jshcrowthe@gmail.com",
    "url": "https://jcrowther.io"
  },
  "files": ["dist"],
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "keywords": ["howto", "modules", "browser", "package.json"],
  "devDependencies": {
    "husky": "^7.0.4",
    "jest": "22.4.1",
    "lint-staged": "^12.0.2",
    "prettier": "1.8.2",
    "rollup": "^2.60.0",
    "typescript": "^4.4.4"
  },
  "scripts": {
    "install": "husky install",
    "lint": "lint-staged",
    "build": "run-p build:esm build:cjs",
    "prettier": "yarn prettier --write",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "repository": "jshcrowthe/howto-browser-modules",
  "license": "MIT",
  "packageManager": "yarn@3.1.0"
}
